{
  "name": "react-gravizo",
  "version": "2.0.2",
  "description": "Generates Graphviz schemas from source code with gravizo.com",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["css", "es", "lib", "umd"],
  "scripts": {
    "build": "nwb build-react-component",
